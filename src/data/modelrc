# This file contains the default initialization for the constants
# used in Suprem IV.  Most of these are the same constants as found
# in Suprem III.  

# Oxidation parameters

#---------- the dry parabolic coeff (singly activated)
oxide  dry orient=111 par.h.0  =12.8667    par.h.e =1.23    p.break=0.

#---------- the dry linear coeff (singly activated) for 111
oxide  dry orient=111 lin.h.0  =1.038e5    lin.h.e=2.0      l.break=0

#---------- thin dry coeffs 
oxide  dry orient=111 thinox.0 =5.87e6     thinox.e =2.32   thinox.l=0.0078
oxide  dry orient=110 thinox.0 =5.37e4     thinox.e =1.80   thinox.l=0.0060
oxide  dry orient=100 thinox.0 =6.57e6     thinox.e =2.37   thinox.l=0.0069

#---------- pressure dependence of dry coefficients
oxide  dry l.pdep  =0.75       p.pdep   =1.0

#---------- the wet parabolic coefficient (double activated)
oxide wet par.l.0 =283.333     par.l.e =1.17
oxide wet par.h.0 =7.00        par.h.e =0.78    p.break=950.

#---------- the wet 111 linear coefficient (doubly activated)
oxide wet orient=111  lin.l.0=3.45e4       lin.l.e=1.60
oxide wet orient=111  lin.h.0=2.95e6       lin.h.e=2.05     l.break=900.

#---------- orientation dependence for B/A
oxide ori.dep = t
oxide orient=100 ori.fac = 0.595
oxide orient=110 ori.fac = 0.833
oxide orient=111 ori.fac = 1.0

#---------- pressure dependence of wet coefficients
oxide wet l.pdep   =1.0        p.pdep   =1.0

#---------- the fermi level dependent B/A rate model.
#           values from Plummer 316 class notes.
oxide baf.dep=t baf.ebk = 241.6 baf.pe=0.46 baf.ppe=1.0 baf.ne=0.145 baf.nne=0.62 baf.k0=2.6e3 baf.ke=1.1

#---------- the chlorine dependence of dry coefficients
oxide wet hcl.pc     =0.00
oxide hclP="0 1 3 5 7 10" hclT="900,1000,1100"
oxide hcl.lin="1,1,1 1.75,1.25,1.621 1.75,1.486,2.207 1.75,1.486,2.207 1.75,1.486,2.207 1.75,1.486,2.207"
oxide hcl.par="1,1,1 1.083,1.658,1.355 1.25,1.840,1.490 1.444,2.075,1.641 1.639,2.332,1.816 2.028,2.759,2.102"

#defaults values of alpha
oxide   oxide /silicon alpha = 2.2

#Henry's Law coefficient
oxide wet henry.coef = 3e19;
oxide dry henry.coef = 5e16;

#number of oxygen atoms in cm3 of oxide
oxide wet theta = 2.2e22;

#mass-transfer; Grove guesstimate 5cm/s * H / Cg
oxide wet oxide /gas trn.0 = 1e6;
oxide dry oxide /gas trn.0 = 1e+3;

#stress-dependent growth coefficients
oxide   Vc=0 Vr=0 Vd=0 Vt=0 Dlim=1.0
oxide   stress.dep = false

# the bogus 1d stuff
oxide   spread = 1.0 initial = 0.0010 mask.edge = -200

# parameters for GPC model
oxide nit.thick=-1 erf.q=0.05 erf.delta=0.04
oxide erf.lbb = (8.25e-3*( 1580.3 - Tox ) * ( Eox ^0.67) * ( eox ^0.3) * exp(-(( en -0.08)^2)/0.06))
oxide erf.h  = (402 * (0.445 - 1.75 * en ) * exp(- Tox /200 ))


#background diffusivity values 
oxide   nitride wet diff.0 = 1.0e-20 diff.e = 0;
oxide   nitride dry diff.0 = 1.0e-20 diff.e = 0;
oxide   poly    wet diff.0 = 1.0e-20 diff.e = 0;
oxide   poly    dry diff.0 = 1.0e-20 diff.e = 0;
oxide   oxyni   wet diff.0 = 1.0e-20 diff.e = 0;
oxide   oxyni   dry diff.0 = 1.0e-20 diff.e = 0;
oxide   silicon wet diff.0 = 1.0e-20 diff.e = 0;
oxide   silicon dry diff.0 = 1.0e-20 diff.e = 0;

#mechanical parameters
#Hetherington (wet) Phys & Chem of Glasses 1964.
material   oxide   visc.0 = 1.99e-7 visc.E = 5.292 visc.x = 0.499 wet
material   oxide   visc.0 = 3.1e-13 visc.E = 7.405 visc.x = 0.499 dry
material   nitride visc.0 = 1.59e10   visc.E=1.12  visc.x = 0.499
material   silicon visc.0 = 1e30   visc.E=0       visc.x = 0.499
material   poly    visc.0 = 5e11   visc.E=0       visc.x = 0.499
material   oxyni   visc.0 = 5e12   visc.E=0       visc.x = 0.499
material   gaas    visc.0 = 1e30   visc.E=0       visc.x = 0.499

#Spinner J. Am. Ceram. Soc. 1956
material   oxide   Young.m = 8.3e11    Poiss.r = 0.2
material   nitride Young.m = 3.89e12 Poiss.r = 0.3
material   silicon Young.m = 1.7e12  Poiss.r = 0.28
material   poly    Young.m = 1.7e11  Poiss.r = 0.28
material   oxyni   Young.m = 3.89e12 Poiss.r = 0.3
material   gaas    Young.m = 7.5e11  Poiss.r = 0.3

#material parameters for thermal expansion and intrinsic stress
#from TPRC
material silicon  lcte="(3.052e-6 + 2 * 6.206e-10 * ( T -293) )"
material oxide    lcte="(1.206e-7 + 2 * 2.543e-10 * ( T -293) )"
material aluminum lcte="(2.438e-5 + 2 * 6.660e-9  * ( T -293) )"
#Tamura & Sunami
material nitride  lcte=3.0e-6
#
material gaas	  lcte="(5.69e-6 + 1.5e-9 * T)"
material gaas	  p.type  act.a="(1)"			act.b="1.00e21"
material gaas	  n.type  act.a="(2.2 - 0.00117 * T)"	act.b="1.25e18"

#older values
#ym[ Si] = 1.5e12;	pr[ Si] = 0.25;
#ym[ SiO2] = 0.66e12;	pr[ SiO2] = 0.17;
#ym[ SiNi] = 3.89e12;	pr[ SiNi] = 0.3;

